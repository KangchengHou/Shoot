   "source": [
    "from glob import glob                                                           \n",
    "import cv2 \n",
    "pngs = glob('./*.JPG')\n",
    "\n",
    "for j in pngs:\n",
    "    img = cv2.imread(j)\n",
    "    cv2.imwrite(j[:-3] + 'png', img)"
   ]
